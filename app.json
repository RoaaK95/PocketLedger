{
  "expo": {
    "name": "PocketLedger",
    "slug": "PocketLedger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wallet.png",
    "scheme": "pocketledger",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/wallet.png",
        "backgroundImage": "./assets/images/wallet.png",
        "monochromeImage": "./assets/images/wallet.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.roaak95.PocketLedger",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/wallet.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wallet.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/wallet.png",
          "color": "#4CAF50",
          "sounds": ["./assets/sounds/notification.wav"],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6504ab0a-cafa-45aa-9464-7ae2c90d13c6"
      }
    },
    "owner": "roaak95"
  }
}
